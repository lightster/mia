<h1>Listing time_logs</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Category</th>
      <th>Description</th>
      <th>Date</th>
      <th>Rounded minutes</th>
      <th>Actual seconds</th>
      <th>Timer enabled</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @time_logs.each do |time_log| %>
      <tr>
        <td><%= time_log.user %></td>
        <td><%= time_log.category %></td>
        <td><%= time_log.description %></td>
        <td><%= time_log.date %></td>
        <td><%= time_log.rounded_minutes %></td>
        <td><%= time_log.actual_seconds %></td>
        <td><%= time_log.timer_enabled %></td>
        <td><%= link_to 'Show', time_log %></td>
        <td><%= link_to 'Edit', edit_time_log_path(time_log) %></td>
        <td><%= link_to 'Destroy', time_log, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Time log', new_time_log_path %>
